let somatorio = 0 
let contador = 0

while (contador < 5){
    let idade = prompt(`Digite a idade da ${contador+1} pessoa`)
    parseFloat(idade)
    somatorio += idade
    contador++
}
let media = (somatorio / 5)
if (media <= 25 ){
    console.log(`A média da turma é jovem  `)
} else if (media >= 26 && media <=60){
    console.log(`A média da turma é Adulta `)
} else if (media > 60){
    console.log(`A média da turma e idosa`)
}
